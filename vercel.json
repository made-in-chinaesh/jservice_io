{
  "version": 2,
  "env": {
    "PORT": 5000,
    "DB_NAME": "jservice",
    "DB_HOST": "localhost",
    "DB_USERNAME": "postgres",
    "DB_PORT": 5432,
    "DB_PASSWORD": "bekaroot"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "POST",
        "GET",
        "PUT",
        "DELETE",
        "PATCH"
      ]
    }
  ]
}